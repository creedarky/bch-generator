// breadcrumb
.breadcrumb
	background transparent
	.date-bread
		float right
		font-size 12px
		color grei-one
//toaster
#toast-container
	top 160px!important
	div.toast
		width 27%!important
		padding 10px 15px 10px 50px!important
		font-size 12px
		margin 0 10px 0 0 
		border-radius 0px!important
		box-shadow 0px 0px 0px #fff
		line-height 19px
		margin 0 auto
		opacity 1!important
		button.toast-close-button
			margin-top 6px
	.toast-error
		background atention-color!important